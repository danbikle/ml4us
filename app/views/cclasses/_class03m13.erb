--
-- fillprices.sql
--

-- This script should Download S&amp;P 500 prices from Yahoo, load them into table called prices.

-- Demo:
-- curl -L ml4.us/csv/GSPC.csv > /tmp/gspc.csv
-- ~ann/madlib_demos/psqlmad -af fillprices.sql

DROP   TABLE IF EXISTS prices;
CREATE TABLE prices
(
cdate   date
,openp  float
,highp  float
,lowp   float
,closep float
,adjp   float
,volume float
);

COPY prices 
(
cdate   
,openp  
,highp  
,lowp   
,closep 
,adjp   
,volume 
)
FROM '/tmp/gspc.csv' WITH CSV HEADER;
</code>
